{
  "name": "chon2",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "release": "echo \"Future version will be:\" ; npm run release:local -- --dry-run | grep \"tagging release\" | cut -d\" \" -f4 | cut -c2- ",
    "release:local": "standard-version",
    "release:publish": "npm run release:local && echo \"You can now « git push --follow-tags github » and « git push --follow-tags origin ».\"",
    "release:publish:all": "npm run release:local && git push --follow-tags github ; git push --follow-tags origin ;"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "4.17.11",
    "register-service-worker": "^1.5.2",
    "vue": "2.5.22",
    "vue-router": "^3.0.2",
    "vuetify": "1.4.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@vue/cli-plugin-babel": "3.3.0",
    "@vue/cli-plugin-eslint": "3.3.0",
    "@vue/cli-plugin-pwa": "3.3.0",
    "@vue/cli-service": "3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^1.0.1",
    "eslint": "5.12.0",
    "eslint-plugin-vue": "5.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-i18n": "8.7.0",
    "vue-template-compiler": "2.5.22"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/max-attributes-per-line": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
